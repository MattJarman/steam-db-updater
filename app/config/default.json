{
  "updater": {
    "toProcess": 30,
    "rate": 800
  },
  "mongodb": {
    "prefix": "mongodb+srv",
    "database": "steam",
    "options": {
      "retryWrites": true,
      "w": "majority"
    }
  },
  "mongoose": {
    "options": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true
    }
  },
  "log": {
    "hierarchy": {
      "ERROR": 0,
      "WARNING": 1,
      "LOG": 2,
      "INFO": 3,
      "DEBUG": 4
    }
  },
  "steam": {
    "store": {
      "baseUrl": "http://store.steampowered.com/api/",
      "routes": {
        "appDetails": "appdetails"
      }
    },
    "api": {
      "baseUrl": "https://api.steampowered.com/",
      "routes": {
        "appList": "ISteamApps/GetAppList/v2"
      }
    }
  }
}
