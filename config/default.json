{
  "updater": {
    "rate": 800,
    "rateIncrease": 60000
  },
  "mongodb": {
    "prefix": "mongodb+srv"
  },
  "mongoose": {
    "options": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true
    }
  },
  "log": {
    "hierarchy": {
      "ERROR": 0,
      "WARNING": 1,
      "LOG": 2,
      "INFO": 3,
      "DEBUG": 4
    }
  }
}
